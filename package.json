{
  "name": "cabral.ro-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build && node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack --config build/webpack.server.config.js --progress --hide-modules",
    "prod": "npm ci && npm run prod:build && cross-env NODE_ENV=production pm2 start server --update-env",
    "prod:build": "cross-env NODE_ENV=production npm run build",
    "lint:js": "./node_modules/.bin/eslint --ignore-pattern '/dist/' --ext .js,.vue ."
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.2.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "jw-paginate": "^1.0.4",
    "lazysizes": "^5.2.0",
    "nprogress": "^0.2.0",
    "photoswipe": "^4.1.3",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.5.0",
    "vanilla-sharing": "^6.0.2",
    "vue": "^2.6.11",
    "vue-meta": "^2.3.2",
    "vue-ripple-directive": "^2.0.1",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vuelidate": "^0.7.5",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/babel-preset-app": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^4.1.6",
    "svgo-loader": "^2.2.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
